{
    "runOnSave.commands": [
        {
            "match": ".*\\.md$",
            "command": "pandoc --filter pandoc-citeproc ${file} --bibliography ${fileDirname}/bib.bib -s -o ${fileDirname}/${fileBasenameNoExtension}.pdf",
            "runIn": "backend"
        }
    ],
    "cSpell.words": [
        "sird"
    ]
}